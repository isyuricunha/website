{
  "name": "@isyuricunha/prettier-config",
  "version": "0.0.15",
  "description": "The prettier config for isyuricunha's projects",
  "license": "MIT",
  "author": "isyuricunha <me@honghong.me> (https://github.com/isyuricunha/)",
  "homepage": "https://github.com/isyuricunha/honghong.me#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isyuricunha/honghong.me.git"
  },
  "bugs": {
    "url": "https://github.com/isyuricunha/honghong.me/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "type-check": "tsc --noEmit"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@isyuricunha/prettier-plugin-package-json": "workspace:*",
    "prettier-plugin-tailwindcss": "^0.6.11"
  },
  "peerDependencies": {
    "prettier": "3.x"
  },
  "devDependencies": {
    "@isyuricunha/eslint-config": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}": "eslint --fix --max-warnings 999"
  }
}
