{
  "name": "@isyuricunha/eslint-config",
  "version": "0.1.34",
  "description": "The eslint config for isyuricunha's projects",
  "license": "MIT",
  "author": "isyuricunha <me@honghong.me> (https://github.com/isyuricunha/)",
  "homepage": "https://github.com/isyuricunha/honghong.me#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isyuricunha/honghong.me.git"
  },
  "bugs": {
    "url": "https://github.com/isyuricunha/honghong.me/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf .turbo dist",
    "dev": "concurrently \"tsup --watch\" \"eslint-config-inspector\"",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "postpublish": "bash ./scripts/update-git-repo.sh",
    "type-check": "tsc --noEmit"
  },
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@eslint-react/eslint-plugin": "^1.53.1",
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^15.5.9",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.4.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "eslint-plugin-testing-library": "^7.15.1",
    "eslint-plugin-turbo": "^2.6.3",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "local-pkg": "^1.1.2"
  },
  "peerDependencies": {
    "eslint": "^9.20.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2",
    "@isyuricunha/tsconfig": "workspace:*",
    "eslint": "^9.39.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{cjs,mjs,js,jsx,cts,mts,ts,tsx}": "eslint --fix --max-warnings 999"
  }
}
