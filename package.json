    "react-ga": "3.3.1",
    "react-hotkeys-hook": "3.4.7",
    "react-icons": "4.6.0",
    "react-palette": "1.0.2",
    "react-query": "3.39.2",
    "react-text-transition": "3.0.2",
    "reading-time": "1.5.0",
    "remark-autolink-headings": "7.0.1",
    "remark-code-titles": "0.1.2",
    "remark-slug": "7.0.1",
    "vcards-js": "2.10.0"
