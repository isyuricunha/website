    "@emotion/styled": "^11.10.4",
    "@mapbox/rehype-prism": "0.8.0",
    "@mdx-js/loader": "2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@next/mdx": "12.3.1",
    "chakra-ui": "^0.3.9",
    "cross-env": "7.0.3",
    "date-fns": "2.29.3",
    "firebase-admin": "11.2.0",
